<?xml version='1.0' encoding='utf-8'?>
<interface version="1.1">
  <object class="tk.Frame" id="_mainf">
    <property name="height">250</property>
    <property name="width">600</property>
    <layout manager="pack">
      <property name="propagate">True</property>
      <property name="side">top</property>
    </layout>
    <child>
      <object class="ttk.Frame" id="mainf">
        <layout manager="pack">
          <property name="propagate">True</property>
          <property name="side">top</property>
        </layout>
        <child>
          <object class="ttk.Frame" id="titlebarf">
            <layout manager="pack">
              <property name="anchor">w</property>
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="ttk.Menubutton" id="tbviewmb">
                <property name="text" translatable="yes">View</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="propagate">True</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Menubutton" id="tbeditmb">
                <property name="text" translatable="yes">Edit</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="propagate">True</property>
                  <property name="side">left</property>
                </layout>
              </object>
            </child>
            <child>
              <object class="ttk.Menubutton" id="tbaboutmb">
                <property name="text" translatable="yes">About</property>
                <layout manager="pack">
                  <property name="anchor">w</property>
                  <property name="propagate">True</property>
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="ttk.Labelframe" id="alldesktopslf">
            <property name="height">200</property>
            <property name="text" translatable="yes">All desktops</property>
            <property name="width">450</property>
            <layout manager="pack">
              <property name="propagate">True</property>
              <property name="side">top</property>
            </layout>
            <child>
              <object class="tk.Scrollbar" id="ldsb">
                <property name="orient">vertical</property>
                <layout manager="pack">
                  <property name="propagate">True</property>
                  <property name="side">right</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="ttk.Combobox" id="langcbb">
        <property name="justify">right</property>
        <layout manager="pack">
          <property name="anchor">e</property>
          <property name="propagate">True</property>
          <property name="side">bottom</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
